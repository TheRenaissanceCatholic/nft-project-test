# Environment Variables for NFT Land Project
# Copy this file to .env and add your actual private key

# Your wallet's private key (keep this secret!)
PRIVATE_KEY=your_private_key_here

# Optional: Alchemy API Key (if different from hardhat.config.js)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Optional: Etherscan API Key (if different from hardhat.config.js)
ETHERSCAN_API_KEY=your_etherscan_api_key_here 